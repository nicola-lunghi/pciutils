WITH_LIBS=
LIBEXT=so
EXEEXT=
LSPCIDIR=$(BINDIR)
LIBZ=-lz
WITH_LIBS+=$(LIBZ)
WITH_LIBS+=-lresolv
PCILIB=$(LIBNAME).$(LIBEXT).$(VERSION)
LDLIBS=
LIB_LDLIBS=$(WITH_LIBS)
PCILIB_LDFLAGS+=-Wl,-soname,$(LIBNAME).$(LIBEXT).$(ABI_VERSION)
PCILIB_LDFLAGS+=-Wl,--version-script=libpci.ver
PCILIBPC=$(LIBNAME).pc
PCIIMPLIB=$(PCILIB)
PCI_CONFIG_H=1
PCI_ARCH_X86_64=1
PCI_OS_LINUX=1
PCI_HAVE_PM_LINUX_SYSFS=1
PCI_HAVE_PM_LINUX_PROC=1
PCI_HAVE_PM_MMIO_CONF=1
PCI_HAVE_PM_ECAM=1
PCI_HAVE_LINUX_BYTEORDER_H=1
PCI_PATH_PROC_BUS_PCI=/proc/bus/pci
PCI_PATH_SYS_BUS_PCI=/sys/bus/pci
PCI_PATH_DEVMEM_DEVICE=/dev/mem
PCI_PATH_ACPI_MCFG=/sys/firmware/acpi/tables/MCFG
PCI_PATH_EFI_SYSTAB=/sys/firmware/efi/systab
PCI_HAVE_PM_INTEL_CONF=1
PCI_HAVE_64BIT_ADDRESS=1
PCI_HAVE_PM_DUMP=1
PCI_COMPRESSED_IDS=1
PCI_IDS=pci.ids.gz
PCI_PATH_IDS_DIR=
PCI_USE_DNS=1
PCI_ID_DOMAIN=pci.id.ucw.cz
PCI_SHARED_LIB=1
PCILIB_VERSION=3.10.0
